{
    "name": "app-template",
    "version": "0.1.0",
    "main": "./dist/bundle.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "bundle": "rollup -c rollup.config.js --bundleConfigAsCjs",
        "bundle:watch": "rollup -c rollup.config.js --bundleConfigAsCjs --watch",
        "generate-types": "tsc -d",
        "examples:build": "webpack",
        "examples:run": "webpack serve --progress",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint": "eslint ./src",
        "test": "jest",
        "prepublish": "npm run generate-types && npm run bundle"
    },
    "files": [
        "/dist",
        "/src"
    ],
    "author": "Robert Corponoi",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/jest": "^29.5.0",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "eslint": "^8.12.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "prettier": "^2.6.1",
        "rollup": "^3.19.1",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.3",
        "webpack": "^5.77.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.13.1"
    }
}
